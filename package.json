{
  "name": "azure_devops_pullrequests",
  "version": "2.5.4",
  "description": "Azure devops chrome extension",
  "scripts": {
    "dev": "webpack-dev-server",
    "start": "webpack --watch --progress",
    "build:gecko": "cross-env BROWSER=gecko yarn _build",
    "build:chrome": "cross-env BROWSER=chrome yarn _build",
    "_build": "yarn webpack:production && yarn zip",
    "build": "yarn build:chrome && yarn build:gecko && yarn zip:source",
    "webpack:production": "cross-env NODE_ENV=production webpack --progress",
    "zip": "node ./zip",
    "zip:source": "node ./zip-source"
  },
  "keywords": [
    "azure",
    "devops",
    "vsts",
    "rest",
    "api",
    "pullRequest",
    "pr"
  ],
